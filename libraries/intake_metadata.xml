	<start_year>
		<label>Start year</label>
		<help>Enter the start year of the project</help>
		<type>text</type>
		<type_configuration>
			<length>4</length>
			<pattern>^[0-9]{4}$</pattern>
			<longtext>false</longtext>
		</type_configuration>
		<required>true</required>
		<depends>false</depends>
	</start_year>
	<end_year>
		<label>End year</label>
		<help>Enter the end year of the project</help>
		<type>text</type>
		<type_configuration>
			<length>4</length>
			<pattern>^[0-9]{4}$</pattern>
			<longtext>false</longtext>
		</type_configuration>
		<required>true</required>
		<depends>false</depends>
	</end_year>
	<depends_example>
		<label>Example</label>
		<help>This field shows how to use the depends object</help>
		<type>text</type>
		<type_configuration>
			<length>10</length>
			<pattern>*</pattern>
			<longtext>false</longtext>
		</type_configuration>
		<required>true</required>
		<depends>
			<action>show</action>
			<if>any</if>
			<fields>
				<option>
					<field_name>start_year</field_name>
					<operator>!=</operator>
					<value>
						<regex>^[0-9]{2}18$</regex>
					</value>
				</option>
				<option>
					<field_name>end_year</field_name>
					<operator>&lt;</operator>
					<value>
						<fixed>2016</fixed>
					</value>
				</option>
			</fields>
		</depends>
	</depends_example>
	<example_checkbox>
		<label>Example checkboxes</label>
