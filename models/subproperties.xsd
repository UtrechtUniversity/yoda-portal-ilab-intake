<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified">
    <xs:element name="metadata">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Project_ID" type="stringNormal" minOccurs="0" maxOccurs="1" />
                <xs:element name="Project_Title" type="stringNormal" minOccurs="0" maxOccurs="1" />
                <xs:element name="Project_Description" type="stringLong" minOccurs="0" maxOccurs="1" />
                <xs:element name="Primary_Investigator" type="stringNormal" minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="PID_Primary_Investigator" type="stringNormal" minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="Project_URL" type="stringURI"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Discipline" type="stringNormal"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="Research_type" type="KindOfDataTypeType"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Ethical_Approval" type="optionsYesNo"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Approval_By" type="stringNormal"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Approval_added" type="optionsYesNo"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Name_approval_doc" type="stringNormal"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="Funding_Organisation" type="stringNormal"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="Fund_Description" type="stringLong"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="Fund_URL" type="stringURI"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="Funder_Role" type="stringNormal"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="Study_Title" type="stringNormal"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Study_Description" type="stringLong"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Study_Purpose" type="stringNormal"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Related_Studies" type="stringNormal"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="URL_Related_Studies" type="stringURI"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="Instrument" type="stringNormal"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="Instrument_Description" type="stringNormal"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="Dataset_Title" type="stringNormal"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Dataset_Description" type="stringLong"  minOccurs="0" maxOccurs="1" />
                <xs:element name="PID_Datapackage" type="stringNormal"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Dataset_Type" type="optionsDatasetType"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Anonymized" type="stringNormal"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Version_dataset" type="stringNormal"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Version_Rationale" type="stringNormal"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Version_Responsibility" type="stringNormal"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Underlying_Dataset" type="stringNormal"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Language_dataset" type="stringNormal"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Owner" type="stringNormal"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="Citation" type="stringNormal"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Data_Source" type="stringNormal"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="Start_Collection_Date" type="stringNormal"  minOccurs="0" maxOccurs="1" />
                <xs:element name="End_Collection_Date" type="stringNormal"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Collection_Method" type="stringNormal"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="Collection_Method_Description" type="stringNormal"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="Sample_Size" type="stringNormal"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Collection_Software" type="stringNormal"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="Creator" minOccurs="0" maxOccurs="1">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Name" type="stringNormal" minOccurs="0" maxOccurs="1"/>
                            <xs:element name="Properties"  minOccurs="0" maxOccurs="1">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="PI" type="stringNormal" minOccurs="0" maxOccurs="1"/>
                                        <xs:element name="PI_Type" type="stringNormal" minOccurs="0" maxOccurs="1"/>
                                        <xs:element name="Affiliation" type="stringNormal" minOccurs="0" maxOccurs="1"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="Contributor" minOccurs="0" maxOccurs="10">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Name" type="stringNormal" minOccurs="0" maxOccurs="1"/>
                            <xs:element name="Properties"  minOccurs="0" maxOccurs="1">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="PI" type="stringNormal" minOccurs="0" maxOccurs="1"/>
                                        <xs:element name="PI_Type" type="stringNormal" minOccurs="0" maxOccurs="1"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="Unit_Analysis" type="stringNormal"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="Location_Covered" type="stringNormal"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="Start_Period_Dataset" type="xs:date"  minOccurs="0" maxOccurs="1" />
                <xs:element name="End_Period_Dataset" type="xs:date"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Name_Contactperson" type="stringNormal"  minOccurs="0" maxOccurs="1" />
                <xs:element name="PID_Contactperson" type="stringNormal"  minOccurs="0" maxOccurs="1" />
                <xs:element name="URL_Contactperson" type="stringURI"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Contact_Info" type="stringNormal"  minOccurs="0" maxOccurs="1" />
                <xs:element name="UU_Dept" type="stringNormal"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="Tag" type="stringNormal"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="Codes_used" type="stringNormal"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="Codes_Acronym" type="stringNormal"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="URL_Code" type="stringNormal"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="Indicator" type="stringNormal"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="Indicator_Acronym" type="stringNormal"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="Indicator_Description" type="stringNormal"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="URL_Indicator" type="stringNormal"  minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="Number_of_files" type="xs:integer"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Retention_period" type="xs:integer"  minOccurs="0" maxOccurs="1" />
                <xs:element name="License" type="stringNormal"  minOccurs="0" maxOccurs="1" />
                <xs:element name="URL_License" type="stringURI"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Publish_Dataset" type="optionsYesNo"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Publish_Metadata" type="optionsYesNo"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Archive" type="optionsYesNo"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Embargo" type="xs:date"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Dataset_Access" type="optionsDatasetAccess"  minOccurs="0" maxOccurs="1" />
                <xs:element name="Dataset_location" type="stringNormal"  minOccurs="0" maxOccurs="1" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:simpleType name="stringURI">
        <xs:restriction base="xs:anyURI">
            <xs:maxLength value="1024"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="stringNormal">
        <xs:restriction base="xs:string">
            <xs:maxLength value="255"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="stringLong">
        <xs:restriction base="xs:string">
            <xs:maxLength value="2700"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="optionsYesNo">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Yes"/>
            <xs:enumeration value="No"/>
            <xs:enumeration value="N.A."/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="optionsDatasetType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Observational Data" />
            <xs:enumeration value="Experimental Data" />
            <xs:enumeration value="Simulation Data" />
            <xs:enumeration value="Derived Data" />
            <xs:enumeration value="Reference Data" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="optionsDatasetAccess">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Open" />
            <xs:enumeration value="Restricted to UU" />
            <xs:enumeration value="Restricted to UU dpt" />
            <xs:enumeration value="Restricted - contact I-lab" />
            <xs:enumeration value="Closed" />
        </xs:restriction>
    </xs:simpleType>

    <!-- Taken from http://www.ddialliance.org/Specification/DDI-Lifecycle/3.2/XMLSchema/FieldLevelDocumentation/schemas/reusable_xsd/simpleTypes/KindOfDataTypeType.html -->
    <xs:simpleType name="KindOfDataTypeType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Qualitative"/>
            <xs:enumeration value="Quantitative"/>
            <xs:enumeration value="Mixed"/>
        </xs:restriction>
    </xs:simpleType>

</xs:schema>